{
  "webroot": "wwwroot",
  "version": "1.0.0-*",
  "userSecretsId": "aspnet-buzz",
  "dependencies": {
    "Microsoft.AspNet.Diagnostics": "1.0.0-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-*",
    "Microsoft.AspNet.SignalR.Server": "3.0.0-*",
    "Microsoft.AspNet.WebSockets.Server": "1.0.0-*",
    "Microsoft.Framework.Logging.Console": "1.0.0-*",
    "Microsoft.Framework.ConfigurationModel.Json": "1.0.0-*",
    "Microsoft.Framework.ConfigurationModel.UserSecrets": "1.0.0-*",
    "Kestrel": "1.0.0-*",
    "RestSharp": "105.0.1"
  },
  "commands": {
    "web": "Microsoft.AspNet.Hosting --server Kestrel --server.urls http://localhost:5004"
  },
  "frameworks": {
    "dnx451": {
        "frameworkAssemblies": {
            "System.Net.Http": ""
        }
    }
  },
  "publishExclude": [
    "node_modules",
    "bower_components"
  ],
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ]
}